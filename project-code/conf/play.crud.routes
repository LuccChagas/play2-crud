# CRUD Routes
# ~~~~

GET		/:model/list		@play.utils.crud.RouterCrudController.list(model: String)
GET     /:model/new         @play.utils.crud.RouterCrudController.newForm(model: String)
POST	/:model/create		@play.utils.crud.RouterCrudController.create(model: String)
GET		/:model/:key		@play.utils.crud.RouterCrudController.show(model: String, key: String)
PUT		/:model/:key		@play.utils.crud.RouterCrudController.update(model: String, key: String)
DELETE	/:model/:key		@play.utils.crud.RouterCrudController.delete(model: String, key: String)
